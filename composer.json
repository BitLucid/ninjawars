{
	"autoload": {
		"psr-4": {
			"model\\": "deploy/model",
			"ChatApp\\": "deploy/lib/control/ChatApp",
			"NinjaWars\\": "deploy/",
			"NinjaWars\\core\\": "deploy/lib/"
		}
	},
	"require": {
		"php": ">=7.1.0",
		"components/jquery": "^2.1.1",
		"rmm5t/jquery-timeago": "^1.6",
		"BitLucid/jquery-linkify": "^2.1",
		"ircmaxell/password-compat": "@stable",
		"swiftmailer/swiftmailer": "~5.4.0",
		"smarty/smarty": "v3.1.33",
		"cboden/ratchet": "^0.4.1",
		"illuminate/database": "~5.4.0",
		"symfony/http-foundation": "^4.1",
		"twbs/bootstrap": "~3.3",
		"pimple/pimple": "~3.0",
		"robmorgan/phinx": "^0.10.1",
		"symfony/polyfill-php73": "^1.9"
	},
	"require-dev": {
		"phpunit/phpunit": "^7.5"
	},
	"config": {
		"component-dir": "deploy/www/components/"
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "rmm5t/jquery-timeago",
				"type": "component",
				"version": "1.4.3",
				"dist": {
					"url": "https://github.com/rmm5t/jquery-timeago/archive/v1.4.3.zip",
					"type": "zip"
				},
				"source": {
					"url": "https://github.com/rmm5t/jquery-timeago.git",
					"type": "git",
					"reference": "v1.4.3"
				},
				"extra": {
					"component": {
						"scripts": [
							"jquery.timeago.js"
						]
					}
				},
				"require": {
					"robloach/component-installer": "*"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "BitLucid/jquery-linkify",
				"type": "component",
				"version": "2.1.1",
				"dist": {
					"url": "https://github.com/BitLucid/jquery-linkify/archive/master.zip",
					"type": "zip"
				},
				"source": {
					"url": "https://github.com/BitLucid/jquery-linkify.git",
					"type": "git",
					"reference": "master"
				},
				"extra": {
					"component": {
						"files": [
							"jquery.linkify.js",
							"jquery-linkify.min.js"
						]
					}
				},
				"require": {
					"robloach/component-installer": "*"
				}
			}
		}
	]
}
